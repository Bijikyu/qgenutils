const {
  default: getContextualTimeout
} = require('./getContextualTimeout');
const {
  default: contextualTimeouts
} = require('./contextualTimeouts');
describe('getContextualTimeout', () => {
  it('should return correct timeout for known operation types', () => {
    expect(getContextualTimeout('llm-inference')).toBe(120000);
    expect(getContextualTimeout('cache-lookup')).toBe(5000);
    expect(getContextualTimeout('database-query')).toBe(45000);
    expect(getContextualTimeout('http-api')).toBe(30000);
    expect(getContextualTimeout('file-upload')).toBe(120000);
  });
  it('should return default timeout for unknown operations', () => {
    expect(getContextualTimeout('unknown-operation')).toBe(15000);
    expect(getContextualTimeout('custom-task')).toBe(15000);
  });
  it('should return default for null/undefined', () => {
    expect(getContextualTimeout(null)).toBe(15000);
    expect(getContextualTimeout(undefined)).toBe(15000);
  });
  it('should match contextualTimeouts values', () => {
    Object.entries(contextualTimeouts).forEach(([op, timeout]) => {
      expect(getContextualTimeout(op)).toBe(timeout);
    });
  });
});
